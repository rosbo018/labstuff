package lab7;

public class Account {
	int money;
	public Account(){
		money = 0;
	}
	public Account(int m){
		money = m;
	}
	public void deposit(int m){
		money+=m;
	}
	public int withdraw(int m) throws NotEnoughMoneyException{
		if (money-m < 0 ){
			throw new NotEnoughMoneyException();
		}
		else{
			return m;
		}
	}

}
